{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "TNO": "Debug"
    }
  },
  "Service": {
    "MaxFailLimit": 5,
    "ApiUrl": "http://host.docker.internal:40010/api"
  },
  "Auth": {
    "Keycloak": {
      "Authority": "http://host.docker.internal:40001/auth/realms/tno",
      "Audience": "tno-service-account",
      "Secret": "{DO NOT STORE SECRET HERE}"
    }
  }
}
